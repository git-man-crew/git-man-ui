<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="gitman.web.controller.SupportedPlatform">
	<App>
		<pages>
			<Page title="{i18n>spPageTitle}">
				<content>
					<Table id="supportedPlatformTable" headerText="{i18n>spTableHeaderText}" items="{supportedPlatforms>/}">
						<items>
							<ColumnListItem type="Active" id="item0" busyIndicatorDelay="1">
								<cells>
									<Text text="{supportedPlatforms>}"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column>
								<header>
									<Label text="{i18n>spNameColumnText}"/>
								</header>
							</Column>
						</columns>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>